@page
@model Friberg_Car_Rentals__Razor_Pages_3_.Pages.Account.LoginModel
@{
    ViewData["Title"] = "Login";
}

<h1 class="main">Login</h1>

<form class="CenterText" method="post">
    <div>
        <label for="username">Username:</label>
        <input type="text" id="username" name="username" />
    </div>
    <div>
        <label for="password">Password:</label>
        <input type="password" id="password" name="password" />
    </div>
    <button type="submit">Login</button>
</form>

<div class="CenterText">@Model.ErrorMessage</div>

<div class="CenterText">
    <a asp-page="/Account/Registration">Register</a>
</div>